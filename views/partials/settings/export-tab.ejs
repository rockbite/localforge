<!-- Export Tab -->
<div class="settings-tab-content" id="export-tab" style="visibility:hidden; position:absolute; z-index:-1">
    <div class="content-scroll-area">
        <div class="settings-section compact">
            <div class="section-header">
                <div class="model-row-label section-title">Export</div>
                <div class="export-actions">
                    <button class="btn select-all-btn">
                        <span class="material-icons">check_box</span> Select All
                    </button>
                    <button class="btn unselect-all-btn">
                        <span class="material-icons">check_box_outline_blank</span> Unselect All
                    </button>
                </div>
            </div>
            
            <div class="export-categories-container">
                <!-- Agents Section -->
                <div class="export-section">
                    <h3>Agents</h3>
                    <div class="export-list agents-export-list">
                        <!-- Agent entries will be dynamically populated by JS -->
                    </div>
                </div>

                <!-- MCPs Section -->
                <div class="export-section">
                    <h3>MCP's</h3>
                    <div class="export-list mcps-export-list">
                        <!-- MCP entries will be dynamically populated by JS -->
                    </div>
                </div>

                <!-- Project Templates Section -->
                <div class="export-section">
                    <h3>Project Templates</h3>
                    <div class="export-list projects-export-list">
                        <!-- Project template entries will be dynamically populated by JS -->
                    </div>
                </div>
                
                <!-- Export Button (moved inside scroll area) -->
                <div class="export-button-container">
                    <button class="btn primary export-btn">
                        <span class="material-icons">download</span>
                        Export
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>